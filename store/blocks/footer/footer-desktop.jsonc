      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/contacto",
        "noFollow": false,
        "text": "Contacto"
      }
    }
  },

  "menu-item#footer-desk-col-2-item-3": {
    "props": {
      "blockClass":"footer-desk-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/tipo-de-pie",
        "noFollow": false,
        "text": "Pisada"

  "flex-layout.col#footer-desk-col-1": {
    "children": [
      "image#footer-col-1-image",
      "rich-text#footer-col-1-text-1",
      "rich-text#footer-col-1-text-2",
      "image-list#footer-col-1-redes"
    ],
    "props": {
      "blockClass": ["footer-desk-col-1"]
    }
  },
  "image#footer-col-1-image": {
    "props": {
      "blockClass": ["image-footer-col-1-image"],
      "src": "/arquivos/Logo-asics-blanco.svg"
    }
  },
  "rich-text#footer-col-1-text-1": {
    "props": {
      "blockClass": ["footer-col-1-text"],
      "text": "**Atención online:**\nasics@inpresspni.com.pe"
    }
  },
  "rich-text#footer-col-1-text-2": {
    "props": {
      "blockClass": ["footer-col-1-text"],
      "text": "**Síguenos en redes**"
    }
  },
  "image-list#footer-col-1-redes": {
    "props": {
      "blockClass":"images-redes",
      "images": [
        {
          "image": "/arquivos/Facebook-mcalidad.svg",
          "mobileImage": "/arquivos/Facebook-mcalidad.svg",
          "link": {
            "url":"https://www.facebook.com/ASICSPeru",
            "newTab": true
          }
        },
        {
          "image": "/arquivos/Instagram-mcalidad.svg",
          "mobileImage": "/arquivos/Instagram-mcalidad.svg",
          "link": {
            "url":"https://www.instagram.com/asicsperu",
            "newTab": true
          }
        }
      ]
    }
  }

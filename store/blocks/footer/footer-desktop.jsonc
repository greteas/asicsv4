{
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "flex-layout.col#footer-desk-col-1",
      "flex-layout.col#footer-desk-col-2",
      "flex-layout.col#footer-desk-col-3",
      "flex-layout.col#footer-desk-col-4"
    ],
    "props": {
      "blockClass": "footer-1-desktop",
      "preserveLayoutOnMobile":true,
      "preventHorizontalStretch":true
    }
  },

  "flex-layout.col#footer-desk-col-4": {
    "children": [
      "image#footer-desk-col-4-image-1",
      "image#footer-desk-col-4-image-2"
    ],
    "props": {
      "blockClass": ["footer-desk-col-4"]
    }
  },
  "image#footer-desk-col-4-image-1": {
    "props": {
      "blockClass": ["footer-desk-col-4-image-1"],
      "src": "/arquivos/icon-libro-reclamaciones.svg",
      "link": {
        "url":"/libro-de-reclamos"
        //"newTab": true
      }
    }
  },
  "image#footer-desk-col-4-image-2": {
    "props": {
      "blockClass": ["footer-desk-col-4-image-2"],
      "src": "/arquivos/Enmascarar grupo 117.svg"
    }
  },

  "flex-layout.col#footer-desk-col-3": {
    "children": [
      "rich-text#footer-desk-col-3-tittle",
      "menu-item#footer-desk-col-3-item-1",
      "menu-item#footer-desk-col-3-item-2",
      "menu-item#footer-desk-col-3-item-3"
    ],
    "props": {
      "blockClass": ["footer-desk-col-3"]
    }
  },
  "rich-text#footer-desk-col-3-tittle": {
    "props": {
      "blockClass": ["footer-desk-tittle"],
      "text": "Atención al cliente"
    }
  },
  "menu-item#footer-desk-col-3-item-1": {
    "props": {
      "blockClass":"footer-desk-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/politicas-de-privacidad",
        "noFollow": false,
        "text": "Políticas de privacidad"
      }
    }
  },
  "menu-item#footer-desk-col-3-item-2": {
    "props": {
      "blockClass":"footer-desk-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/terminos-y-condiciones",
        "noFollow": false,
        "text": "Términos y condiciones"
      }
    }
  },
  "menu-item#footer-desk-col-3-item-3": {
    "props": {
      "blockClass":"footer-desk-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/preguntas-frecuentes",
        "noFollow": false,
        "text": "Preguntas frecuentes"
      }
    }
  },

  "flex-layout.col#footer-desk-col-2": {
    "children": [
      "rich-text#footer-desk-col-2-tittle",
      "menu-item#footer-desk-col-2-item-1",
      "menu-item#footer-desk-col-2-item-2",
      "menu-item#footer-desk-col-2-item-3"
    ],
    "props": {
      "blockClass": ["footer-desk-col-2"]
    }
  },
  "rich-text#footer-desk-col-2-tittle": {
    "props": {
      "blockClass": ["footer-desk-tittle"],
      "text": "Sobre Asics"
    }
  },
  "menu-item#footer-desk-col-2-item-1": {
    "props": {
      "blockClass":"footer-desk-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/nosotros",
        "noFollow": false,
        "text": "Nosotros"
      }
    }
  },
  "menu-item#footer-desk-col-2-item-2": {
    "props": {
      "blockClass":"footer-desk-item",

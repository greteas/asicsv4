{
  "store.home": {
    "blocks": [
      "SwitchVisibility",
      "list-context.image-list#banner-principal",
      "__fold__",
      //"__fold__.experimentalLazyAssets",
      "flex-layout.row#mas-vendidos-texts",
      "flex-layout.row#mas-vendido-productos",
      "rich-text#mejora-rendimiento",
      "flex-layout.row#mejora-rendimiento-row",
      "rich-text#porque-comprar",
      "flex-layout.row#porque-comprar-content",
      "flex-layout.row#newletter-content"
    ]
  },

  "flex-layout.row#newletter-content": {
    "title":"NEWSLETTER",
    "children": ["newsletter-form#home"],
    "props": {
      "blockClass": ["newletter-content"]
    }
  },
  "newsletter-form#home": {
    "children": [
      "rich-text#newsletter-text",
      "flex-layout.row#newsletter-mail-submit",
      "flex-layout.row#newsletter-checkbox-confirmation"
    ]
  },
  "flex-layout.row#newsletter-checkbox-confirmation": {
    "children": [
      "newsletter-checkbox-confirmation",
      "rich-text#newsletter-checkbox-confirmation-text"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "blockClass": ["newsletter-checkbox-confirmation"]
    }
  },
  "rich-text#newsletter-checkbox-confirmation-text": {
    "props": {
      "blockClass": ["newsletter-checkbox-confirmation-text"],
      "text": "Acepto los [Términos y Condiciones](/terminos-y-condiciones/?target=_blank) y la [Política de Protección de Datos Personales.](https://www.youtube.com/?target=_blank)"
    }
  },
  "flex-layout.row#newsletter-mail-submit": {
    "children": [
      "newsletter-input-email",
      "newsletter-submit"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile":true,
      "blockClass": ["newsletter-mail-submit"]
    }
  },
  "newsletter-input-email":{
    "props": {
      "placeholderText":"Ingresa tu correo electrónico"
    }
  },
  "newsletter-submit":{
    "props":{
      "submitButtonLabel":"Suscríbete"
    }
  },
  "rich-text#newsletter-text": {
    "props": {
      "blockClass": ["newsletter-text"],
      "text": "¡SÉ UN EXPERTO EN RUNNING CON ASICS!\n**Suscríbete para novedades, información, descuentos y más.**"
    }
  },


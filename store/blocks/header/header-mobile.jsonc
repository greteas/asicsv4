    "props": {
      "blockClass":"footer-mob-menu-item",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "/",
        "noFollow": false,
        "text": "Lanzamientos"
      }
    }
  },



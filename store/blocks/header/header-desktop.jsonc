  "rich-text#1-desktop-text-1": {
    "props": {
      "blockClass": ["1-desktop-text-1"],
      "text": "TOP BAR: PROMOCIONES - OFERTAS - ANUNCIOS"
    }
  },
  "rich-text#1-desktop-text-2": {
    "props": {
      "blockClass": ["1-desktop-text-2"],
      "text": "Atención al cliente: **(01) 993 483 539**"
    }
  },


  "flex-layout.col#spacer": {
    "props": {
      "width": "grow"
    }
  },
  "sticky-layout#2-desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": ["flex-layout.row#2-desktop"]
  },
  "flex-layout.row#2-desktop": {
    "title":"Header - 2",
    "props": {
      "blockClass": "main-header",
      "horizontalAlign": "center",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "flex-layout.col#logo-desktop",
      "flex-layout.col#category-menu",
      "flex-layout.col#spacer",
      "search-bar#search-desplegable-desktop",
      "login#desktop",
      "minicart.v2"
    ]
  },
  "login#desktop": {
    "props": {
      "showIconProfile":true,
      "iconLabel":"Mi cuenta"
    }
  },
  "icon#login":{
    "props":{
      "viewBox":"0 0 23 23",
      "size":"23",
      "id":"hpa-profile",
      "handle":"caretIcon"
    }
  },
  "search-bar#search-desplegable-desktop": {
    "blocks": ["autocomplete-result-list.v2#desktop"],
    "props": {
      "blockClass": ["search-desplegable-desktop"],
      "openAutocompleteOnFocus": true,
      "placeholder":"¿Qué deseas buscar?"
    }
  },

  "autocomplete-result-list.v2#desktop": {
    "props": {
      "maxSuggestedProducts":2,
      "blockClass": "autocomplete-result-list-desktop",
      "maxTopSearches":0,
      "maxSuggestedTerms":3,
      "productLayout":"VERTICAL"
    },
    "blocks": ["product-summary.shelf"]
  },


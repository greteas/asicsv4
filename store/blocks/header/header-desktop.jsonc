{
  "flex-layout.row#1-desktop": {
    "title": "Header - 1",
    "children": [
      //"SwitchVisibility",
      //"modal-trigger#popup-home",
      "flex-layout.row#1-desktop-content",
      "stylesGlobal"
    ],
    "props": {
      "blockClass":"1-desktop",
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true
    }
  },

  "SwitchVisibility":{
    "title":"Visibilidad del Popup",
    "props": {
      "visibility": true
    },
    "children": ["modal-trigger#popup-home"]
  },

//**

  "modal-trigger#popup-home": {
    "title": "POPUP",
    "props":{
      "trigger":"load-session",
      "blockClass":"popup-home-trigger"
    },
    "children": [
      "modal-layout#popup-home"
    ]
  },

  "modal-layout#popup-home": {
    "children": [
      "modal-header#boton",
      "flex-layout.row#popup-home-content"
    ],
    "props":{
      "blockClass":"popup-home-layout"
    }
  },

  "flex-layout.row#popup-home-content": {
    "children": [
      "image-list#popup-home-image",
      "flex-layout.row#form-content-flex"
      //"form#popup-home-form"
    ],
    "props": {
      "blockClass": ["popup-home-content"]
    }
  },
  "flex-layout.row#form-content-flex": {
    "children": ["form#popup-home-form"],
    "props": {
      "blockClass": ["form-content-flex"]
    }
  },

  "image-list#popup-home-image": {
    "props": {
      "blockClass":"popup-home-image",
      "preload": true,
      "height": 443,
      "maxHeight": 443,
      "images": [
        {
          "image": "/arquivos/popup-home-desk.jpg",
          "mobileImage":"/arquivos/popup-home-mob.png"
        }
      ]
    }
  },

  "form#popup-home-form":{
    "props": {
      "blockClass":"popup-home-form-form",
      "entity": "PH",
      "schema": "schema_PopupHomeAsics"
    },
    "children": [
      "rich-text#popup-home-col-text",
      "form-input.text#correo-popup-home-form",
      "flex-layout.row#terminos-popup-form-flex",
      "form-submit#submit-popup-home-form"
    ],
    "blocks": ["form-success#popup-home-success"]
  },
  "flex-layout.row#terminos-popup-form-flex": {
    "children": [
      "form-field-group#terminos-popup-home-form",
      "rich-text#newsletter-checkbox-confirmation-text"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "blockClass": ["terminos-contacto-form-flex"]
    }
  },

  "form-field-group#terminos-popup-home-form": {
    "props": {
      "blockClass": ["terminos-popup-home-field"],
      "pointer": "#/properties/terminosycondiciones",
      "label": "display none"
    }
  },
  "form-input.text#correo-popup-home-form": {
    "props": {
      "blockClass":["correo-popup-home-input"],
      "pointer": "#/properties/correo",
      "placeholder":"Ingresa tu correo electrónico"
    }
  },
  "form-success#popup-home-success": {
    "props":{
      "blockClass":"popup-home-success-form"
    },
    "children": [
      "rich-text#popup-home-succes"
    ]
  },
  "rich-text#popup-home-succes": {
    "props": {
      "blockClass":["popup-home-succes-text"],
      "text": "Gracias por registrarte",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "form-submit#submit-popup-home-form": {
    "props": {
      "blockClass":["popup-home-form-submit"],
      "label": "Suscríbete"
    }
  },
  "rich-text#popup-home-col-text": {
    "props": {
      "blockClass": ["popup-home-col-text"],
      "text": "Descubre nuevos productos y obtén\n**15% DSCTO**\n*En tu primera compra*"
    }
  },



  "modal-header#boton":{
    "props":{
      "showCloseButton": true
    }
  },

